{
  "name": "phoole/di",
  "type": "library",
  "license": "Apache-2.0",
  "version": "1.0.7",
  "description": "Dependency injection for Phoole",
  "keywords": [
    "phoole",
    "library",
    "php",
    "di",
    "dependency injection"
  ],
  "authors": [
    {
      "name": "Hong Zhang",
      "homepage": "https://github.com/phoole"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "psr/container": "^1.0",
    "phoole/config": "^1.0.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "provide": {
    "psr/container-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Phoole\\Di\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phoole\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit",
    "check": "phpcs --standard=psr12 src/",
    "format": "phpcbf --standard=psr12 src/"
  }
}